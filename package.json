{
  "name": "loco-fast-store-admin",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm run watch:css",
    "build": "npm run build:css",
    "watch:css": "tailwindcss -i ./assets/static/css/app.css -o ./assets/static/css/output.css --watch",
    "build:css": "NODE_ENV=production tailwindcss -i ./assets/static/css/app.css -o ./assets/static/css/output.css --minify"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "tailwindcss": "^3.4.0"
  }
}
